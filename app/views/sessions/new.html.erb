<div align="center">
  <h1>Krazy Kalf Klothing</h1></div>
 
  <%= form_tag url: "/sessions" do %>
    <p>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </p>

    <p>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </p>

    <p><%= submit_tag 'Log In' %> or <%= link_to 'Create Account', new_customer_path %></p>
    
  <% end %>
